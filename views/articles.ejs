<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>

    <h1>All Articles</h1>
    <hr>
    <div style="background-color: bisque; color: black; padding: 20px;">
    <% articlesList.forEach(function(article) { %>
        <div style="border: 1px solid black; margin-bottom: 10px; padding: 10px;">
            <h2><%= article.title %></h2>
            <p><%= article.content %></p>
            <p>Likes: <%= article.numberOfLikes %></p>
        </div>
    <% }) %>
    </div>
    
</body>
</html>